    <path
        android:pathData="M14,14
                          L54,14
                          L54,42
                          L28,68
                          L14,68
                          Z"
        android:fillColor="#FFFFFF"/>

    <!-- Lower-right solid block -->
    <path
        android:pathData="M54,66
                          L80,40
                          L94,40
                          L94,94
                          L54,94
                          Z"
        android:fillColor="#FFFFFF"/>

    <!--
    The NEGATIVE SPACE between these two shapes
    forms an abstract upward-pointing arrow/path.
    This is the hidden meaning - discovered upon reflection.
    -->

    <!-- Small accent: floating element suggesting destination -->
    <path
        android:pathData="M78,22 L86,22 L86,30 L78,30 Z"
        android:fillColor="#FFFFFF"
